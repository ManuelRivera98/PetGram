{
"version": 2,
"builds": [
    { "use": "@now/static-build", "src": "package.json"}
],
"routes": [
    {
      "src": "(.*).js", "dest": "/$1.js"
    },
    {
      "src": "(.*).json", "dest": "/$1.json"
    },
    {
      "src": "(.*).jsx", "dest": "/$1.jsx"
    },
    {
      "src": "(.*).png", "dest": "/$1.png"
    },
    {
      "src": "/.*", "dest": "/public/index.html"
    }
]
}